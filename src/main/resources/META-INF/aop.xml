<!DOCTYPE aspectj PUBLIC
        "-//AspectJ//DTD//EN" "http://www.eclipse.org/aspectj/dtd/aspectj.dtd">
<aspectj>
	<weaver
		options="-Xset:weaveJavaxPackages=true -verbose -Xreweavable">
		<!--  -showWeaveInfo -debug  -->
		<include
			within="java.lang.Object org.springframework.beans.factory.config.*" />
		<include within="org.springframework" />
		<include within="org.apache.log4j.spi.*" />
		<include within="definitions..*" />
		<exclude within="settings.SpringBeanLogging" />
		<exclude within="definitions.structures.abstr..*" />
		<exclude within="plotter..*" />
	</weaver>
	<aspects>
		 <aspect name="definitions.cache.CachingAspect" />
		 <aspect name="definitions.TestAspect" />
		 <aspect name="definitions.DistributionCollector" />
		 <aspect name="definitions.PointcutSearch" />
		 <aspect name="definitions.DeepSearch" />
	</aspects>
</aspectj>